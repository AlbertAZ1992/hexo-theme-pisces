<div class="pisces-tags-cloud" >
    <blockquote class="tags-comments">
    <% if(theme.categories_comment){ %>
        分类命名规范：
        <% for(var i in theme.categories_comment){ %>
        <li><%= theme.categories_comment[i] %></li>
        <% } %>
    <% } %>
    </blockquote>
    <div id="JS_tag_cloud" class="tags-cloud">
    <% site.tags.sort('name').each(function(item){ %>
        <a href="<%- config.root %><%- item.path %>" title="<%= item.name %>" data-num="<%= item.posts.length %>"><%= item.name %></a>
    <% }); %>
    </div>
</div>
<%- partial('_partial/comment',{item: page}) %>
